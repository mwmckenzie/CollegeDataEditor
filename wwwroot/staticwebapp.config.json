{
  

  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad?post_login_redirect_uri=.referrer",
      "statusCode": 302
    }
  },
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/ProgramBrowser",
      "allowedRoles": ["authenticated","editor","admin"]
    },
    {
      "route": "/ProgramEditorView",
      "allowedRoles": ["editor","admin"]
    },
    {
      "route": "/CitizenshipEditorView",
      "allowedRoles": ["editor","admin"]
    },
    {
      "route": "/OrgTypeEditorView",
      "allowedRoles": ["editor","admin"]
    },
    {
      "route": "/ProgramTypeEditorView",
      "allowedRoles": ["editor","admin"]
    },
    {
      "route": "/ResidenceEditorView",
      "allowedRoles": ["editor","admin"]
    },
    {
      "route": "/SubjectEditorView",
      "allowedRoles": ["editor","admin"]
    },
    {
      "route": "/TagEditorView",
      "allowedRoles": ["editor","admin"]
    },
    {
      "route": "/TopicEditorView",
      "allowedRoles": ["editor","admin"]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    }
    ]
}